pct_03da = zoo::rollmean(Share, k = 3, fill = NA),
pct_04da = zoo::rollmean(Share, k = 4, fill = NA),
pct_05da = zoo::rollmean(Share, k = 5, fill = NA)) %>%
dplyr::ungroup()
#chart
ggplot(DTPoll, mapping = aes(x=Date, y=pct_03da, color=Opinion))+
