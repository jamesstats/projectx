library(dplyr)
data <- data.frame(
year = c(2018, 2019, 2020, 2021, 2022),
value = c(100, 120, 130, 110, 150)
)
ggplot(data, aes(x = year, y = value)) +
geom_line() +
geom_point() +
labs(title = "Change in Value Over Years",
x = "Year",
y = "Value") +
theme_minimal()
data <- data %>%
mutate(change = value - lag(value))
ggplot(data, aes(x = year, y = value)) +
geom_line() +
geom_point() +
geom_text(aes(label = round(change, 1)),
vjust = -1.5,
color = "red") +
labs(title = "Change in Value Over Years",
x = "Year",
y = "Value") +
theme_minimal()
